{
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "vue/eqeqeq": "error",
        "vue/html-indent": [
            "error",
            4
        ],
        "vue/attribute-hyphenation": [
            "error",
            "never"
        ]
    }
}